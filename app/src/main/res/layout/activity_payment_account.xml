<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/myprofile"
    tools:context=".paymentAccount">

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="133dp"
        android:layout_height="139dp"
        android:layout_marginTop="92dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/myac"
        android:contentDescription="TODO" />

    <TextView
        android:id="@+id/cno"
        android:layout_width="235dp"
        android:layout_height="48dp"
        android:layout_marginTop="20dp"
        android:fontFamily="sans-serif-black"
        android:hint="@string/card_number"
        android:textColor="#0C0808"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.926"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/rid" />

    <TextView
        android:id="@+id/nd"
        android:layout_width="102dp"
        android:layout_height="33dp"
        android:fontFamily="sans-serif-black"
        android:hint="@string/name1"
        android:textColor="#0A0505"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.555"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView5" />

    <TextView
        android:id="@+id/Cvcd"
        android:layout_width="235dp"
        android:layout_height="48dp"
        android:layout_marginTop="12dp"
        android:fontFamily="sans-serif-black"
        android:hint="@string/cvc1"
        android:textColor="#0C0707"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.926"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/cno" />

    <TextView
        android:id="@+id/dd"
        android:layout_width="235dp"
        android:layout_height="48dp"
        android:layout_marginTop="12dp"
        android:fontFamily="sans-serif-black"
        android:hint="@string/experitation_date1"
        android:textColor="#0E0707"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.909"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Cvcd"
        app:layout_constraintVertical_bias="0.036" />

    <TextView
        android:id="@+id/textView35"
        android:layout_width="224dp"
        android:layout_height="40dp"
        android:layout_marginTop="56dp"
        android:text="@string/welcome_to_paradise_hotel1"
        android:textColor="#996515"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.556"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/nd" />

    <Button
        android:id="@+id/button"
        android:layout_width="204dp"
        android:layout_height="40dp"
        android:background="@drawable/button"
        android:onClick="pay2"
        android:text="@string/change_your_details1"
        android:textColor="@color/white"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.531"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/dd"
        app:layout_constraintVertical_bias="0.542" />

    <TextView
        android:id="@+id/textView36"
        android:layout_width="89dp"
        android:layout_height="22dp"
        android:layout_marginTop="72dp"
        android:fontFamily="sans-serif-black"
        android:text="@string/registration_id1"
        android:textColor="#090606"
        app:layout_constraintEnd_toStartOf="@+id/rid"
        app:layout_constraintHorizontal_bias="0.472"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView35" />

    <TextView
        android:id="@+id/textView37"
        android:layout_width="92dp"
        android:layout_height="23dp"
        android:layout_marginTop="40dp"
        android:fontFamily="sans-serif-black"
        android:text="@string/card_number1"
        android:textColor="#0A0303"
        app:layout_constraintEnd_toStartOf="@+id/cno"
        app:layout_constraintHorizontal_bias="0.45"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView36" />

    <TextView
        android:id="@+id/textView38"
        android:layout_width="84dp"
        android:layout_height="19dp"
        android:layout_marginTop="32dp"
        android:fontFamily="sans-serif-black"
        android:text="@string/cvc1"
        android:textColor="#0C0606"
        app:layout_constraintEnd_toStartOf="@+id/Cvcd"
        app:layout_constraintHorizontal_bias="0.405"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView37" />

    <TextView
        android:id="@+id/textView39"
        android:layout_width="78dp"
        android:layout_height="20dp"
        android:layout_marginTop="28dp"
        android:fontFamily="sans-serif-black"
        android:text="@string/ex_date1"
        android:textColor="#0A0505"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/dd"
        app:layout_constraintHorizontal_bias="0.39"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView38"
        app:layout_constraintVertical_bias="0.04000002" />

    <EditText
        android:id="@+id/rid"
        android:layout_width="232dp"
        android:layout_height="48dp"
        android:layout_marginTop="56dp"
        android:layout_marginEnd="28dp"
        android:background="@drawable/inputfield"
        android:ems="10"
        android:hint="@string/register_id"
        android:inputType="textPersonName"
        android:textColorHint="@color/Darkblue"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView35"
        android:autofillHints="" />

    <Button
        android:id="@+id/button10"
        android:layout_width="49dp"
        android:layout_height="29dp"
        android:background="@drawable/button"
        android:text="@string/button55"
        android:textColor="@color/white"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.516" />
</androidx.constraintlayout.widget.ConstraintLayout>